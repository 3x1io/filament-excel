{
    "name": "3x1io/filament-excel",
    "description": "Excel Export for Resources",
    "keywords": [
        "laravel",
        "excel",
        "import",
        "export",
        "ui",
        "filament"
    ],
    "homepage": "https://3x1.io",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "io3x1\\FilamentExcel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "3x1",
            "email": "info@3x1.io",
            "role": "CEO"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.9",
        "maatwebsite/excel": "^3.1",
        "spatie/laravel-package-tools": "^1.10"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "io3x1\\FilamentExcel\\FilamentExcelProvider"
            ]
        }
    },
    "prefer-stable": true
}
